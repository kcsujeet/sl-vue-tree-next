{
    "name": "sl-vue-tree-next",
    "version": "0.0.6",
    "description": "draggable tree vue component",
    "browserslist": "> 0.25%, not dead",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kcsujeet/sl-vue-tree-next"
    },
    "files": [
        "dist",
        "src/components"
    ],
    "main": "dist/sl-vue-tree-next.umd.cjs",
    "module": "dist/sl-vue-tree-next.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/sl-vue-tree-next.js",
            "require": "./dist/sl-vue-tree-next.umd.cjs"
        },
        "./sl-vue-tree-next-minimal.css": {
            "import": "./dist/sl-vue-tree-next-minimal.css",
            "require": "./dist/sl-vue-tree-next-minimal.css"
        },
        "./sl-vue-tree-next-dark.css": {
            "import": "./dist/sl-vue-tree-next-dark.css",
            "require": "./dist/sl-vue-tree-next-dark.css"
        }
    },
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "serve": "vite preview",
        "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
        "format": "prettier .  --write"
    },
    "keywords": [
        "vue",
        "vue3",
        "tree",
        "draggable",
        "sortable"
    ],
    "author": "Holiber",
    "license": "MIT",
    "peerDependencies": {
        "vue": "^3.0.0"
    },
    "devDependencies": {
        "@vitejs/plugin-vue": "^5.0.3",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-vue": "^9.21.1",
        "prettier": "3.2.4",
        "rimraf": "^2.6.2",
        "typescript": "^5.3.3",
        "vite": "^5.0.12",
        "vite-plugin-dts": "^3.7.2",
        "vue": "^3.4.15"
    }
}
